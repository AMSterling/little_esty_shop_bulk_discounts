<%= render partial: 'shared/merchant_links', locals: {path: "/merchant/#{@merchant.id}"} %>

<body>
  <div class="row">
    <h2 class='col-12'>My Invoices</h2>
  </div>

  <% @invoices.each do |invoice| %>
    <p><%= link_to "Invoice ##{invoice.id}", merchant_invoice_path(@merchant, invoice) %></p>
  <% end %>
</body>
